<%= turbo_stream.replace dom_id(@product_match) do %>
  <tr id="<%= dom_id(@product_match) %>" class="bg-red-50/30 hover:bg-gray-50 opacity-50">
    <td class="px-4 py-3 sticky left-0 bg-inherit z-10">
      <div class="text-sm font-medium text-gray-500 line-through"><%= @product_match.display_name %></div>
      <div class="text-xs text-red-500">Rejected</div>
    </td>
    <% @aggregated_list.supplier_lists.includes(:supplier).map(&:supplier).uniq.each do |supplier| %>
      <td class="px-4 py-3">
        <% item = @product_match.item_for_supplier(supplier) %>
        <% if item %>
          <div class="text-sm text-gray-400"><%= item.name.truncate(60) %></div>
        <% end %>
      </td>
    <% end %>
    <td class="px-4 py-3 text-center">
      <span class="text-xs text-red-500">Rejected</span>
    </td>
  </tr>
<% end %>
